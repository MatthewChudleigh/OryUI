@model KratosUiNode

@{
    var attributes = Model.Attributes.GetKratosUiNodeInputAttributes();
    var uiText = attributes.Label ?? Model.Meta.Label;
}

<div class="field">
    <p>Unsupported Input Type: "@attributes.Type, Label: "@uiText?.Text"</p>
    @foreach (var message in Model.Messages)
    {
        <KratosUiTextMessage Message="@message"/>
    }
</div>